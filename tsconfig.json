{
    "compilerOptions": {
      "noEmit": true,
      "module": "NodeNext",
      "target": "ESNext",
      "moduleResolution": "NodeNext",
      "resolveJsonModule": true,
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,

      "noUnusedLocals": true,
      "noUnusedParameters": true
    },
  }